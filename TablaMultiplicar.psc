Proceso TablaMultiplicar
	Definir NMultiplicar, Contador Como Entero;
	Escribir "Escriba el numero a sacar la tabla de multiplicar";
	Leer NMultiplicar;
	Contador<-0;
	
	Mientras Contador<=10 Hacer
		
		Escribir Contador," por ", NMultiplicar," es: ", Contador*NMultiplicar;
		
		Contador<-Contador + 1;
		
	FinMientras
	
	
	
	
FinProceso
